# n8n Portable Configuration - Optimized
# Generated and optimized for n8n v1.111.1

# Basic Configuration
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http

# Data Storage
N8N_USER_FOLDER=./data
DB_TYPE=sqlite
DB_SQLITE_DATABASE=./data/database/n8n.sqlite
DB_SQLITE_POOL_SIZE=5

# Security Configuration
N8N_SECURE_COOKIE=false
N8N_ENCRYPTION_KEY=portable-n8n-key-change-this-in-production

# Task Runners (New feature)
N8N_RUNNERS_ENABLED=true

# Logging Configuration
N8N_LOG_LEVEL=info
N8N_LOG_OUTPUT=file
N8N_LOG_FILE_LOCATION=./data/logs

# UI Configuration
N8N_DISABLE_UI=false

# Workflow Configuration
N8N_DEFAULT_BINARY_DATA_MODE=filesystem
N8N_BINARY_DATA_STORAGE_PATH=./data/binary-data

# Performance Optimization
N8N_BINARY_DATA_TTL=1440

# License Configuration (Disable license checks for portable version)
N8N_LICENSE_TENANT_ID=
N8N_LICENSE_ACTIVATION_KEY=

# Metrics and Analytics (Disable for portable version)
N8N_DIAGNOSTICS_ENABLED=false
N8N_VERSION_NOTIFICATIONS_ENABLED=false

# Email Configuration (Optional - for testing workflows)
# N8N_SMTP_HOST=
# N8N_SMTP_PORT=587
# N8N_SMTP_USER=
# N8N_SMTP_PASS=
# N8N_SMTP_SENDER=

# Webhook Configuration
WEBHOOK_URL=http://localhost:5678/
WEBHOOK_TUNNEL_URL=